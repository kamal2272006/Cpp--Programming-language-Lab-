#include <iostream> 
using namespace std; 
void swap(int &x, int &y) { 
    int temp = x; 
    x = y; 
    y = temp; 
} 
int main() { 
    cout << "Roll No: 24B11442" << endl; 
    int a = 60; 
    int b = 70;                                                                                     Page No.:                   
    cout << "Before swap: a = " << a << " b = " << b << endl; 
    swap(a, b); 
    cout << "After swap: a = " << a << " b = " << b << endl; 
    return 0; 
} 
